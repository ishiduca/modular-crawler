{
  "crawler_config": {
    "request": {
      "timout": 5000,
      "interval": 800,
      "error": "./error.log",
      "access": "./access.log"
    },
    "export_xml": "../../source/export.xml",
    "lock": 3,
    "ignore_txt": "./ignore.txt",
    "different": {
      "tmp": {
        "tmpDir": "./tmp",
        "expires": "30d"
      }
    }
  },
  "version": "0.0.1",
  "name": "modular-crawler-single",
  "dependencies": {
    "mkdirp": "^0.5.0"
  }
}
